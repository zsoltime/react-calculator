{
  "env": {
    "browser": true,
    "mocha": true,
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
  },
  "plugins": [
    "react",
  ],
  "rules": {
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"],
    }],
  },
}
